<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="registerPane" prefHeight="400.0"
	prefWidth="600.0" styleClass="root"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="kr.or.nextit.register.RegisterController">
	<children>
		<ImageView fitHeight="400.0" fitWidth="610.0" layoutX="-8.0"
			pickOnBounds="true">
			<image>
				<Image url="@/kr/or/nextit/register/rank02.png" />
			</image>
		</ImageView>
		<Label id="welcome-text" alignment="CENTER"
			contentDisplay="CENTER" layoutX="161.0" layoutY="51.0"
			prefHeight="43.0" prefWidth="281.0" text="RANK 등록"
			textAlignment="CENTER" textFill="#f8dc06">
			<font>
				<Font name="System Bold" size="35.0" />
			</font>
		</Label>
		<TextField fx:id="nameField" layoutX="172.0" layoutY="171.0"
			prefHeight="43.0" prefWidth="258.0" promptText="이름">
			<font>
				<Font size="17.0" />
			</font>
		</TextField>
		<Button layoutX="125.0" layoutY="278.0" mnemonicParsing="false"
			onAction="#registerAction" prefHeight="32.0" prefWidth="90.0"
			styleClass="button" text="등록" wrapText="true">
			<font>
				<Font size="22.0" />
			</font>
		</Button>
		<Button layoutX="386.0" layoutY="278.0" mnemonicParsing="false"
			onAction="#cancelAction" prefHeight="32.0" prefWidth="90.0"
			styleClass="button" text="취소" wrapText="true">
			<font>
				<Font size="22.0" />
			</font>
		</Button>
		<Label alignment="CENTER" layoutX="261.0" layoutY="350.0"
			prefHeight="36.0" prefWidth="82.0"
			style="-fx-background-color: black;" text="주식회사선영" textFill="#3c3c3c" />
	</children>
</AnchorPane>
