<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="400.0"
	xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/8.0.171"
	fx:controller="kr.or.nextit.rank.RankController">
	<children>
		<ImageView fitHeight="418.0" fitWidth="400.0"
			layoutY="183.0" pickOnBounds="true">
			<image>
				<Image url="@grass.jpg" />
			</image>
		</ImageView>
		<ImageView fitHeight="600.0" fitWidth="400.0"
			pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@world1.png" />
			</image>
		</ImageView>
		<TableView fx:id="tableView" layoutX="10.0" layoutY="77.0"
			prefHeight="447.0" prefWidth="380.0">
			<columns>
				<TableColumn fx:id="ranking" minWidth="50.0"
					prefWidth="84.0" text="Ranking" />
				<TableColumn fx:id="name" minWidth="20.0"
					prefWidth="175.0" text="Name" />
				<TableColumn fx:id="count" minWidth="45.0"
					prefWidth="120.0" text="Count" />
			</columns>
		</TableView>
		<Button fx:id="closeBtn" layoutX="292.0" layoutY="537.0" mnemonicParsing="false"
			onAction="#closeAction" prefHeight="38.0" prefWidth="77.0" text="닫기">
			<font>
				<Font size="19.0" />
			</font>
		</Button>
		<Label alignment="CENTER" layoutX="131.0" layoutY="21.0"
			text="RANKING">
			<font>
				<Font name="System Bold" size="31.0" />
			</font>
		</Label>
		<ImageView fitHeight="91.0" fitWidth="99.0" layoutX="10.0"
			layoutY="509.0" pickOnBounds="true" preserveRatio="true"
			rotate="-11.3">
			<image>
				<Image url="@rank2.png" />
			</image>
		</ImageView>
	</children>
</AnchorPane>
